package fundamentos;

import java.util.Scanner;

public class Wrapper {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		//Para converter para uma classe primeira letra maiuscula
		Scanner entrada = new Scanner(System.in);
		
		byte a =100;
		Byte b = 100;
		short r = 1000;
		Short s = 1000;
		int h = 10000;
		long k = 100000;
		Long l = 100000L;
		Integer i = 10000;
		
		System.out.println(b.byteValue() + a);
		System.out.println(s.toString() + r);
		System.out.println(i.compareTo(h));
		System.out.println(l.longValue() + k);
		
		System.out.println("Digite um nro: ");
		Integer nro = 3*Integer.parseInt(entrada.nextLine());
		System.out.printf("O numero digitado vezes o nro 3 = %d ",nro);
		
		
	
		
		
	
		
		entrada.close();
	}

}
