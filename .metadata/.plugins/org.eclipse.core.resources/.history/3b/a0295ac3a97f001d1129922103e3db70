package fundamentos;

public class TipoString {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println("Ola - Seja Bem vindo".charAt(6));
		
		String s = "Boa tarde";
		s = s.toUpperCase();
		System.out.println(s);
		System.out.println(s.concat(" !!!"));
		
	}
}

